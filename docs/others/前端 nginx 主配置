前端 nginx 主配置
##Start.

user  www www;
worker_processes auto;
worker_rlimit_nofile 65535;

error_log  var/logs/error.log;
pid        var/logs/nginx.pid;

events {
    use epoll;
    worker_connections  65535;
}

http {
    server_tokens off;
    include       nginx.mime.types;  
    default_type  application/octet-stream;
    sendfile on;

    #map $http_x_forwarded_for $clientip {
    #        default $http_x_forwarded_for;
    #        ""      $remote_addr;
    #}

    set_real_ip_from  10.0.0.0/8;
    set_real_ip_from  172.16.0.0/16;
    real_ip_header X-Forwarded-For;

    map $remote_addr $clientip {
            default $remote_addr;
    }


    log_format  main  '$http_host $remote_addr ${request_time}s - [$time_local] '
                      '"$request" $status $body_bytes_sent '
                      '"$http_referer" - "SUP=$cookie_SUP SUBP=$cookie_SUBP" "REQUEST_ID=$request_uid" "$http_user_agent"';

    log_format  5xxerr  '$http_host $remote_addr ${request_time}s - [$time_local] '
                      '"$request" $status $body_bytes_sent '
                      '"$http_referer" - "SUP=$cookie_SUP" "REQUEST_ID=$request_uid" "$http_user_agent" "$server_addr"';

    log_format  mobile  '[$time_local]`$http_x_up_calling_line_id`"$request"`"$http_user_agent"`$staTus`[$remote_addr]`$sent_http_x_weibo_uid`"$http_referer"`$request_time`$body_bytes_sent`$http_x_forwarded_proto`$http_x_forwarded_for`$request_uid`$http_host`$http_cookie`$upstream_response_time`xd';

        log_format  hongbao  '$http_host $remote_addr ${request_time}s - [$time_local] '
                          '"$request" $status $body_bytes_sent '
                          '"$http_referer" - "X-Log-Uid=$http_x_log_uid SUP=$cookie_SUP SUBP=$cookie_SUBP SUB=$cookie_SUB" "REQUEST_ID=$request_uid" "$http_user_agent"';

        log_format video '$http_host $remote_addr ${request_time}s - [$time_local] ' '"$request" $status $body_bytes_sent ' '"$http_referer" - "SUP=$cookie_SUP SUBP=$cookie_SUBP" "REQUEST_ID=$request_uid" "$http_user_agent" $upstream_response_time' ;

        log_format  zhuzhan  '$http_host $remote_addr ${request_time}s - [$time_local] '
                      '"$request" $status $body_bytes_sent '
                      '"$http_referer" - "SUP=$cookie_SUP SUBP=$cookie_SUBP" "REQUEST_ID=$request_uid" "$http_user_agent" "REQ_UID=$http_x_uid"';

    add_header DPOOL_HEADER $hostname;
    keepalive_timeout  0;
    lingering_timeout 50ms;

    client_header_buffer_size 8k;
    client_max_body_size 120m;
    client_body_timeout 10s;
    client_header_timeout 5s;
    large_client_header_buffers 8 4k;
    server_names_hash_bucket_size 128;

    open_file_cache max=65535 inactive=20s;
    open_file_cache_valid 30s;
    open_file_cache_min_uses 3;

    fastcgi_connect_timeout 300;
    fastcgi_send_timeout 300;
    fastcgi_read_timeout 300;
    fastcgi_buffer_size 4k;
    fastcgi_buffers 256 4k;
    fastcgi_busy_buffers_size 8k;
    fastcgi_max_temp_file_size 0;

        fastcgi_temp_path /dev/shm/nginx_tmp;
        fastcgi_cache_path /dev/shm/fastcgi_cache levels=1:2 keys_zone=common_cache:20m inactive=5m max_size=1024m;
        fastcgi_cache_key "$request_method$host$request_uri";
        fastcgi_cache_min_uses 1;
        fastcgi_cache_bypass $cookie_nocache $arg_nocache;
        fastcgi_no_cache $cookie_nocache $arg_nocache;
        fastcgi_cache_use_stale error timeout http_500 http_404;
        fastcgi_cache_methods GET HEAD POST;

        proxy_temp_path /dev/shm/nginx_tmp;
        proxy_cache_path /dev/shm/proxy_cache levels=1:2 keys_zone=common_proxy:20m inactive=5m max_size=1024m;
        proxy_cache_key "$request_method$host$request_uri";
        proxy_cache_min_uses 1;
        proxy_cache_bypass $cookie_nocache $arg_nocache;
        proxy_no_cache $cookie_nocache $arg_nocache;
        proxy_cache_use_stale error timeout http_500 http_404;
        proxy_cache_methods GET HEAD POST;  

    gzip on;
    #gzip_flush_mode SYNC_FLUSH;
    gzip_min_length 1k;
    gzip_buffers 4 4k;
    gzip_http_version 1.1;
    gzip_comp_level 9;
    gzip_types text/plain text/css text/xml application/x-javascript application/json text/javascript;
    gzip_vary on;

    include vhost/*.conf;
}